{
    "api_keys": {
        "claude": "your-claude-api-key-here"
    },
    "paths": {
        "workspace": {
            "code": "workspace/code",
            "data": "workspace/data",
            "output": "workspace/output"
        }
    },
    "execution": {
        "timeout": 30,
        "max_retries": 3
    },
    "memory": {
        "mongo_uri": "mongodb://localhost:27017/",
        "database": "hivemind"
    },
    "messaging": {
        "rabbitmq_uri": "amqp://localhost:5672/"
    }
}
